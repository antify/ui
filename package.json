{
  "name": "@antify/ui",
  "version": "2.4.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "storybook dev -p 6006 --no-open",
    "build": "unbuild",
    "prepack": "unbuild",
    "type-check": "vue-tsc --build --force",
    "build-storybook": "storybook build",
    "release": "standard-version && git push --follow-tags && pnpm publish --access public",
    "chromatic": "npx chromatic"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "style": "./dist/index.css",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./install": {
      "types": "./dist/install.d.ts",
      "import": "./dist/install.mjs",
      "require": "./dist/install.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "tailwindcss": "4.0.7",
    "vue": "^3.4.29"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@tiptap/extension-heading": "^2.11.5",
    "@tiptap/extension-underline": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@vuepic/vue-datepicker": "^6.1.0",
    "@vueuse/components": "^10.11.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/router": "^10.11.1",
    "big.js": "^6.2.2"
  },
  "devDependencies": {
    "@antify/validate": "^1.14.0",
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/js": "^9.22.0",
    "@faker-js/faker": "^8.4.1",
    "@nuxt/eslint-plugin": "^1.2.0",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/vue3": "^8.6.4",
    "@storybook/vue3-vite": "^8.6.4",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@tailwindcss/vite": "4.0.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/big.js": "^6.2.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.24",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "chromatic": "^11.27.0",
    "eslint-plugin-storybook": "^0.11.4",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "jsdom": "^24.1.3",
    "npm-run-all2": "^6.2.6",
    "standard-version": "^9.5.0",
    "storybook": "^8.6.4",
    "storybook-vue3-router": "^5.0.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.26.1",
    "unbuild": "^2.0.0",
    "vite": "^5.4.14",
    "vitest": "^1.6.1",
    "vue-tsc": "^2.2.8"
  }
}
